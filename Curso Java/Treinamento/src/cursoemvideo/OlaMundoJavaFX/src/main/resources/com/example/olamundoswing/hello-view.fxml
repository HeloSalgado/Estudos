<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" prefHeight="353.0" prefWidth="307.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.olamundoswing.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
     <Label fx:id="welcomeText" opacity="0.82" text="Vai aparecer uma mensagem" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Bookman Old Style" size="17.0" />
         </font>
     </Label>
     <Button fx:id="btnCliqueMe" onAction="#clicouBotao" text="Clique em mim" textFill="#cf0c0c">
         <font>
            <Font size="19.0" />
         </font>
       </Button>
   </children>

</VBox>
